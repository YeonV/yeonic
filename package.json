{
  "name": "yeonic",
  "private": true,
  "version": "0.0.3",
  "type": "module",
  "author": {
    "name": "YeonV aka Blade",
    "email": "dev@yeonv.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/YeonV/yeonic"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "buildgh": "tsc && vite build --base=/yeonic/",
    "build-electron": "tsc && vite build && tsc --project electron/tsconfig.json && rimraf ./electron/build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "predesktop": "tsc && vite build && npx cap sync @capacitor-community/electron",
    "desktop": "npx cap open @capacitor-community/electron",
    "desktop:dev": "node ./electron/live-runner.js",
    "premobile": "tsc && vite build && npx cap sync",
    "mobile": "run-script-os",
    "mobile:darwin": "npx cap run ios --livereload --external --target=66DD2162-E00C-4B8A-9426-4AC75CE957D1 --watch",
    "mobile:default": "npx cap sync android && npx cap run android --livereload --external -p 3000"
  },
  "dependencies": {
    "@capacitor-community/electron": "^5.0.1",
    "@capacitor/android": "^5.6.0",
    "@capacitor/core": "^5.6.0",
    "@capacitor/device": "^5.0.7",
    "@capacitor/ios": "^5.6.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.15.7",
    "@mui/material": "^5.15.7",
    "capacitor-zeroconf": "^2.0.0",
    "electron-reload": "^2.0.0-alpha.1",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "zustand": "^4.5.0"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.4",
    "@capacitor/cli": "^5.6.0",
    "@redux-devtools/extension": "^3.3.0",
    "@types/react": "^18.2.53",
    "@types/react-dom": "^18.2.18",
    "@types/react-native-zeroconf": "^0.12.3",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "run-script-os": "^1.1.6",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  },
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    },
    "electron": {
      "src": "electron"
    }
  }
}